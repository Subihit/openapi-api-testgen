{
  "name": "openapi-api-testgen",
  "version": "0.1.0",
  "type": "module",
  "description": "Generate deterministic, editable Supertest API tests from OpenAPI specifications",
  "main": "./dist/index.js",
  "files": ["dist"],
  "bin": {
    "openapi-api-testgen": "./dist/cli.js"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "dev": "node --loader ts-node/esm src/cli.ts"
  },
 "keywords": [
  "openapi",
  "testing",
  "supertest",
  "api",
  "test-generation"
 ],
"author": "Subihit Hazra",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^12.1.0",
    "openapi-types": "^12.1.3"
  }
}
